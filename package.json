    "description": "A unit tester for Universalis.",
    "author": "Ayrton Sparling",
    "private": true,
    "license": "UNLICENSED",
    "engines": {
        "node": ">=7.0.0"
    },
    "bin": {
        "universalis-tester": "dist/universalis-tester-cli.js"
    },
    "scripts": {
        "dev": "webpack-cli --mode=development --watch",
